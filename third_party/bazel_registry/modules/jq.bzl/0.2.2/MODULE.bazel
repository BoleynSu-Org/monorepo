module(
    name = 'jq.bzl',
    version = '0.2.2',
)

bazel_dep(
    name = 'aspect_bazel_lib',
    version = '2.21.2',
)

bazel_dep(
    name = 'bazel_skylib',
    version = '1.8.1',
)

bazel_dep(
    name = 'platforms',
    version = '1.0.0',
)

toolchains = use_extension(
    '@aspect_bazel_lib//lib:extensions.bzl',
    'toolchains',
)

use_repo(
    toolchains,
    'jq_toolchains',
)

register_toolchains(
    '@jq_toolchains//:all',
)

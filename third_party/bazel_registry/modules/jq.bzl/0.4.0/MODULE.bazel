module(
    name = 'jq.bzl',
    version = '0.4.0',
)

bazel_dep(
    name = 'bazel_lib',
    version = '3.0.0',
)

bazel_dep(
    name = 'package_metadata',
    version = '0.0.6',
)

bazel_dep(
    name = 'platforms',
    version = '1.0.0',
)

toolchains = use_extension(
    '@jq.bzl//jq:extensions.bzl',
    'toolchains',
)

use_repo(
    toolchains,
    'jq_toolchains',
)

register_toolchains(
    '@jq_toolchains//:all',
)

module(
    name = 'tar.bzl',
    version = '0.5.6',
)

bazel_dep(
    name = 'bazel_features',
    version = '1.34.0',
)

bazel_dep(
    name = 'aspect_bazel_lib',
    version = '2.21.1',
)

bazel_dep(
    name = 'bazel_skylib',
    version = '1.8.1',
)

bazel_dep(
    name = 'platforms',
    version = '1.0.0',
)

toolchains = use_extension(
    '//tar:extensions.bzl',
    'toolchains',
)

use_repo(
    toolchains,
    'bsd_tar_toolchains',
)

register_toolchains(
    '@bsd_tar_toolchains//:all',
)

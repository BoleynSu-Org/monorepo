module(
    name = 'tar.bzl',
    version = '0.9.0',
)

bazel_dep(
    name = 'bazel_features',
    version = '1.41.0',
)

bazel_dep(
    name = 'bazel_lib',
    version = '3.2.0',
)

bazel_dep(
    name = 'bazel_skylib',
    version = '1.9.0',
)

bazel_dep(
    name = 'platforms',
    version = '1.0.0',
)

toolchains = use_extension(
    '//tar:extensions.bzl',
    'toolchains',
)

use_repo(
    toolchains,
    'bsd_tar_toolchains',
)

register_toolchains(
    '@bsd_tar_toolchains//:all',
)
